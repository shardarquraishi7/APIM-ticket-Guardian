{
  "name": "kevin-cfw-nextjs-starter-ui",
  "version": "0.1.0",
  "description": "Next.js Cloudflare Workers Starter Kit with TELUS styling for experimentation and prototyping",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "opennextjs-cloudflare build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "setup": "node scripts/setup.js",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:staging": "opennextjs-cloudflare build && wrangler deploy --env staging",
    "deploy:prototype": "opennextjs-cloudflare build && wrangler deploy --env prototype",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "db:create": "wrangler d1 create starter-kit-db",
    "db:migrate": "wrangler d1 execute starter-kit-db --file=./database/schema.sql",
    "db:seed": "wrangler d1 execute starter-kit-db --file=./database/seed.sql",
    "r2:create": "wrangler r2 bucket create starter-kit-files"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250607.0",
    "@hookform/resolvers": "^5.1.1",
    "@opennextjs/cloudflare": "^1.2.1",
    "chart.js": "^4.4.9",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "styled-components": "^6.1.18",
    "wrangler": "^4.19.1",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
